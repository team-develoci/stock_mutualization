            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Stock Mutualization">
                <display-name xml:lang="x-default">Stock Mutualization</display-name>
                <attribute attribute-id="SM_InventoryID"/>
